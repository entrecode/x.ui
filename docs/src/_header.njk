<header class="demo-navbar is-fixed is-positioned-nw is-layer-200 has-transition" data-col="12">
  <div class="toolbar toolbar_small">
    <div data-col="2">
      <button :class="{ 'is-active': menuActive }" @click.prevent="menuActive = !menuActive" class="hamburger-btn"></button>
    </div>
    <div data-col="8">
      <div class="toolbar__title toolbar__title_center">
        <a href="#top">
          <img src="assets/xui-logo-small.svg" alt="x.ui - atomic css framwork">
        </a>
      </div>
    </div>
    <div data-col="2">
      <nav class="nav">
        <ul class="nav__items">
          <li class="nav__item" style="padding-right: 16px;">
            <small class="is-ink-lighter is-nowrap">v. {{ version }}</small>
          </li>
          <li class="nav__divider"></li>
          <li class="nav__item">
            <a href="https://github.com/entrecode/x.ui" class="btn btn_clear btn_minor"><span class="ec-icon github"></span></a>
          </li>
        </ul>
      </nav>
    </div>
  </div>
</header>